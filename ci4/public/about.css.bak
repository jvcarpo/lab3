*{
	margin: 0;
	padding: 0;
}
.container{
	height:100%;
	width: 100%;
	background-image: url(images/sky.png);
	background-position:center;
	background-repeat: no-repeat;
	background-size: cover;
}
.navigation{
	width:100%;
	height:15vh;
	margin: auto;
	display: flex;
	align-items: center;
	}
nav ul li{
	display: inline-block;
	list-style: none;
	margin: 0px 30px;
}
nav ul li a{
	text-decoration: none;
	color: black;
}
.about{
	text-align: center;
}

.me{
	background-color:white;
	height:500px;
	width:500px;
}
.paragraph{
	background-color:white;
	height:500px;
	width:900px;
}
.division{
	float:left;
	margin-top: 20px;
	margin-left:20px;
	margin-right:20px;
	border-radius:24px;
	border: 2px solid #000000;
}
div p{
	margin-top: 10px;
	margin-left:20px;
	margin-right:20px;
	text-align: justify;
	font-size:23;
}
.myself{
	width:395px;
	height:395px;
	margin-right:50px;
	margin-left:50px;
	margin-top:50px;
	margin-bottom:50px;
	display:block;
}